package string.Ramana_Sir;

public class Prgm6 {

    public static void main(String[] args) {
        String s = "ash#123";
        int count = 0;
        for (int i = 0; i < s.length(); i++) {
            if (Character.isDigit(s.charAt(i))) {
                count += s.charAt(i) - '0';
            }
        }
        System.out.println(count);

    }

}
