class RemoveDup {
   public static void main(String args[]){
        String s="malayalam";
        char[] c=s.toCharArray();
        
        for(int i=0;i<s.length();i++){
            for(int j=i+1;j<s.length();j++){
                if(c[i]==c[j]){
                    c[j]='\0';
                }
            }
        }
        for(char res:c){
            System.out.print(res);
        }
    }
  }
