class btod{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the input");
        int n=sc.nextInt();
        int result = 0, i = 0;
        while (n != 0) {
            int dup = n % 10;
            result += dup * Math.pow(2, i);
            n = n / 10;
            i++;
        }
        System.out.println(result);
    }
}
