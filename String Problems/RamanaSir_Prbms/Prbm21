package string.Ramana_Sir;

public class Prbm21 {

    public static void main(String args[]) {

        String s = "easy very is java";
        String arr[] = s.split(" ");
        StringBuilder sb = new StringBuilder();

        for (int i = arr.length - 1; i >= 0; i--) {
            sb.append(arr[i] + " ");
        }
       
        System.out.println(sb.toString().trim());
    }

}
