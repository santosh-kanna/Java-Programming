package stream.api;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Stream {                                         // Removing Duplicate Characters

    public static void Stream4() {
        
        String s = "jaaavvaa";
        s.chars().distinct().mapToObj(i->(char)i).forEach(i->System.out.println(i));

    }

    public static void main(String args[]) {
        Stream4();
    }
}
