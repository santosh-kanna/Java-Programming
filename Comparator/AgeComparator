import java.util.Comparator;


public class AgeComparator implements Comparator<Student> {

    public int compare(Student s1, Student s2) {
        if (s1.Age > s2.Age) {
            return 1;
        } 
        else if (s1.Age < s2.Age) {
            return -1;
        } 
        else {
            return 0;
        }
    }

}
