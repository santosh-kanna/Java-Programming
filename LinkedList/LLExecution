public class LLexecution {

    public static void main(String[] args) {

        SingleLinkedList l = new SingleLinkedList();
        l.add(10);
        l.add(20);
        l.add(30);
        l.add(40);
        l.add(50);
        
       l.insert(5,70);
        System.out.println(l.size());
       for(int i=0;i<l.size();i++){
           System.out.print(l.get(i)+" ");
       }
    }

}
